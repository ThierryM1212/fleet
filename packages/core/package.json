{
  "name": "@fleet-sdk/core",
  "version": "0.1.3",
  "description": "Fleet SDK core library",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "sideEffects": false,
  "repository": "fleet-sdk/fleet",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "keywords": [
    "ergo",
    "blockchain",
    "transactions",
    "serialization"
  ],
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@fleet-sdk/common": "workspace:^",
    "@fleet-sdk/crypto": "workspace:^",
    "@fleet-sdk/serializer": "workspace:^"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "src",
    "dist",
    "!**/*.spec.*",
    "!**/*.json",
    "!tests",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ]
}
